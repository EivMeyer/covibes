{
  "name": "covibes-server",
  "version": "1.0.0",
  "description": "CoVibe TypeScript Express Server",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development nodemon src/server.ts",
    "dev:src": "nodemon src/server.ts",
    "dev:development": "NODE_ENV=development nodemon src/server.ts",
    "dev:production": "NODE_ENV=production nodemon src/server.ts",
    "build": "tsc",
    "build:development": "NODE_ENV=development tsc",
    "build:production": "NODE_ENV=production tsc",
    "start": "node dist/server.js",
    "start:development": "NODE_ENV=development node dist/server.js",
    "start:production": "NODE_ENV=production node dist/server.js",
    "lint": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "typescript",
    "express",
    "collaboration",
    "ai-agents"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "@prisma/client": "^5.14.0",
    "ansi-regex": "^6.2.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dockerode": "^4.0.7",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.2",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "node-pty": "^1.0.0",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "simple-git": "^3.28.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "ssh2": "^1.17.0",
    "strip-ansi": "^7.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/dockerode": "^3.3.42",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.10",
    "@types/passport": "^1.0.17",
    "@types/passport-github2": "^1.2.9",
    "@types/ssh2": "^1.15.0",
    "@types/supertest": "^6.0.3",
    "bcrypt": "^6.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prisma": "^5.14.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
